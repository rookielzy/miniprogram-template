.module_carousel, .module_products, .module_search {
  margin-bottom: 20rpx;
}

.module_carousel {
  width: 100%;
  height: 380rpx;
}

.module_products {
  width: 100%;
  display: flex;
  flex-flow: row;
  justify-content: space-around;
  align-items: center;
}

.module_search {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
